[flake8]
max-line-length = 88
exclude =
    .git,
    __pycache__,
    migrations,
    manage.py,
    settings.py,
    .vscode,
    .idea,
    .pytest_cache,
    .mypy_cache,
    .tox,
    .eggs,
    .gitignore,
    env,
    venv,
    .venv,
    *.egg-info,
    build,
    dist
ignore =
    E203,
    W503
show-source = True
count = True